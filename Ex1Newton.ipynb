    "%load_ext autoreload\n",
    "%autoreload 2\n",
    "\n",
    "import Newton\n",
    "# Exemplo: f(x) = x^2 - 2\n",
    "def f(x):\n",
    "    return x**2 - 2\n",
    "\n",
    "def df(x):\n",
    "    return 2 * x\n",
    "\n",
    "# Chute inicial\n",
    "x0 = 0.5\n",
    "\n",
    "# Executa o método\n",
    "raiz = Newton.newton(f, df, x0)\n",
    "print(f\"Raiz aproximada: {raiz}\")\n"
   ]
